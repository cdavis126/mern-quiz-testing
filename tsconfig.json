{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "types": ["cypress", "node"],
    "strict": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "moduleDetection": "force"
  },
  "include": ["client", "server", "cypress"]
}

